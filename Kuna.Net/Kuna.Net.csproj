<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>c# api wrapper for kuna.io crypto exchange</Description>
    <Copyright>ridicoulous</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ridicoulous/Kuna.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ridicoulous/Kuna.Net</RepositoryUrl>
    <PackageTags>kuna.io kuna</PackageTags>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <Authors>ridicoulous</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>orders management</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CryptoExchange.Net" Version="2.0.13" />
    <PackageReference Include="Pusher.NetStandard" Version="0.5.2" />
  </ItemGroup>

</Project>
