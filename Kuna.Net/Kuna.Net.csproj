<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Version>1.8.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>c# api wrapper for kuna.io crypto exchange</Description>
    <Copyright>ridicoulous</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ridicoulous/Kuna.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ridicoulous/Kuna.Net</RepositoryUrl>
    <PackageTags>kuna.io kuna</PackageTags>
    <AssemblyVersion>1.6.7.0</AssemblyVersion>
    <FileVersion>1.6.7.0</FileVersion>
    <Authors>ridicoulous</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>account info update</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CryptoExchange.Net" Version="3.0.14" />
    <PackageReference Include="Pusher.NetStandard" Version="0.5.9" />
  </ItemGroup>

</Project>
